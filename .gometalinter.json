{
    "Enable": ["deadcode", "golint", "vet", "gotype", "gocyclo", "maligned", "errcheck", "misspell", "staticcheck"],
    "Exclude": ["test", "vendor", "fake_*", "charts"],
    "Format": "[{{.Severity}}]: {{.Path}}: {{.Line}}:{{if .Col}}{{.Col}}{{end}}: {{.Message}} ({{.Linter}})",
    "Deadline": "300s",
    "Aggregate": true,
    "Sort": ["severity", "path", "line", "message"],
    "Checkstyle": false
}
